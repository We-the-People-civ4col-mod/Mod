    <TEXT>
        <Tag>TXT_KEY_PROFESSION_BIRD_HUNTER</Tag>
        <English>
            <Text>Bird Hunter:Bird Hunters</Text>
            <Gender>Male</Gender>
            <Plural>0:1</Plural>
